alias aliases='code ~/.zsh_aliases'
alias k9s='XDG_CONFIG_HOME=~/ k9s'
alias scripts='jq ".scripts" package.json'
alias deps='jq ".dependencies" package.json'
alias rmf='rm -rf'
alias notes='code ~/notes'
alias stash='git --no-pager stash'
alias dockers='docker ps -a --format "table {{.Names}}\t{{.Image}}\t{{.RunningFor}}\t{{.Status}}"'
alias mongosh='docker run -it --rm mongosh mongosh'
alias gitl="git log --graph --abbrev-commit --decorate --pretty=format:'%h - %an : %s'"